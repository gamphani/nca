<%= simple_form_for(@facility) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :facility_name %>
    <%= f.input :proprieter %>
    <%= f.input :district %>
    <%= f.input :cluster %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
